extends ../../app/pug/layouts/service-layout.pug

append vars
  - title = "Ai"
  - description = "Описание Ai"

append content
    +regPage('ai')
    +regComponent('header', true, true)
    +regBlock('collapse-stages', true, true)
    //- регистрация
    //- +regLibrary('lib-name/lib.css', 'lib-name/lib.js')
    //- +regBlock('block-name', true, true)
    +header().light
    main.page
        .page__container
            .page__content
                .ai
                    .ai-main
                        .ai-main__bool.ai-bool
                            video(autoplay, playsinline, loop, muted, poster="./images/ai/bool-poster.jpg")
                                source(src="./images/ai/bool.mp4" type="video/mp4")
                        .ai-main__wave.ai-wave
                            img(src="./images/ai/ai-wave2.png", alt="").mobile
                            img(src="./images/ai/ai-wave.png", alt="")
                        .ai-main__container.container
                            .ai-main__body
                                .ai-main__descritption
                                    h1.h1 Разработка и&nbsp;интеграция AI-решений
                                    p
                                        | Автоматизируем бизнес-процессы, внедряя In-house AI-архитектуру.
                                        br
                                        | Снижаем операционные расходы и&nbsp;преодолеваем ограничения.
                                    button(data-modal="consultation").btn.primary.medium Обсудить проект
                                ul.ai-main__adv
                                    li
                                        span
                                            | до
                                            |
                                            span 55%
                                        p выполнение задач быстрее
                                    li
                                        span
                                            | на
                                            |
                                            span 29%
                                        p поиск информации быстрее
                                    li
                                        span
                                            | до
                                            |
                                            span 35%
                                        p повышение продуктивности
                                    li
                                        span
                                            | на
                                            |
                                            span 12%
                                        p увеличение конверсии



                    .ai-examples
                        .ai-examples__container.container
                            .ai-examples__body
                                .ai-examples__header
                                    p.h2 Примеры внедрений AI-ассистентов, которые помогли расширить возможности бизнеса и команды
                                    p.description AI обрабатывает массивы данных, которые невозможно охватить вручную. Создаем гибридные рабочие процессы: алгоритмы обеспечивают скорость и режим 24/7, а люди — экспертность, эмпатию и гибкость
                                .ai-examples__grid
                                    .ai-examples__types
                                        ul
                                            li
                                                button(type="button", data-type="realty").ai-examples__type.active
                                                    span
                                                        +svg("crane")
                                                    p Недвижимость
                                            li
                                                button(type="button", data-type="e-com").ai-examples__type
                                                    span
                                                        +svg("cart")
                                                    p E-com
                                            li
                                                button(type="button", data-type="factory").ai-examples__type
                                                    span
                                                        +svg("factory2")
                                                    p Производство
                                            li
                                                button(type="button", data-type="b2b").ai-examples__type
                                                    span
                                                        +svg("case2")
                                                    p B2B
                                            li
                                                button(type="button", data-type="medicine").ai-examples__type
                                                    span
                                                        +svg("heartbeat")
                                                    p Медицина
                                            li
                                                button(type="button", data-type="logistics").ai-examples__type
                                                    span
                                                        +svg("box-arrow")
                                                    p Логистика
                                            li
                                                button(type="button", data-type="services").ai-examples__type
                                                    span
                                                        +svg("service-bell")
                                                    p Услуги
                                            li
                                                button(type="button", data-type="consulting").ai-examples__type
                                                    span
                                                        +svg("compare")
                                                    p Консалтинг
                                            li
                                                button(type="button", data-type="it-telecom").ai-examples__type
                                                    span
                                                        +svg("broadcust")
                                                    p IT и Телеком

                                    .ai-examples__cards
                                        .ai-examples__category(data-category="realty", style="display:block;").active
                                            .ai-examples__category--inner
                                                .ai-case
                                                    .ai-case__body
                                                        .ai-case__top
                                                            .ai-case__logo
                                                                img(src="./images/case/iva.png", alt="")
                                                            p.ai-case__title
                                                                | Чат-бот для техподдержки, рассчитанный на&nbsp;100000 звонков в&nbsp;неделю
                                                            ul.ai-case__list
                                                                li
                                                                    span Клиент
                                                                    p Логистическая компания: 500 сотрудников, 1800 шт - парк машин
                                                                li
                                                                    span Ситуация
                                                                    p Клиент терял до 10% клиентов в год из-за недовольства работой службы поддержи и долгого ожидания ответа при обращении на горячую линию
                                                                li
                                                                    span Решение
                                                                    p Голосовой бот, работающий с помощтю RAG технологии интегрированный в CRM-систему и считывающий статус, трек номер и что-то еще
                                                        .ai-case__bottom
                                                            .ai-case__results
