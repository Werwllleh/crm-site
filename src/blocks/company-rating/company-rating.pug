                .company-rating__years_items
                    buton(data-filter-year="2025").company-rating__button-year.active 2025
                    span.separate /
                    buton(data-filter-year="2024").company-rating__button-year 2024
                    span.separate /
                    buton(data-filter-year="2023").company-rating__button-year 2023
                    span.separate /
                    buton(data-filter-year="2022").company-rating__button-year 2022
